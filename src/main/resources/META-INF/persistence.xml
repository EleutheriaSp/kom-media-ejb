<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	
	<persistence-unit name="kom-media-ejb" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>java:jboss/datasources/KomMedia</jta-data-source>
		
		<class>pl.kompro.jpa.rozrachunki.StatusNaleznosciPro</class>
		<class>pl.kompro.jpa.rozrachunki.StatusRozrachunkuPro</class>
		
		<class>pl.kommedia.jpa.administracja.FirmaApp</class>
		<class>pl.kommedia.jpa.kartoteki.DzialSprzedazyKart</class>
		<class>pl.kommedia.jpa.kartoteki.DzialanoscGospodarczaKart</class>
		<class>pl.kommedia.jpa.kartoteki.OsobaFizycznaKart</class>
		<class>pl.kommedia.jpa.kartoteki.PodmiotKart</class>
		<class>pl.kommedia.jpa.kartoteki.PodmiotKartotekiKart</class>
		<class>pl.kommedia.jpa.kartoteki.PodmiotPowiazanyKart</class>
		<class>pl.kommedia.jpa.kartoteki.PracownikKart</class>
		<class>pl.kommedia.jpa.rozrachunki.PrzychodPro</class>
		<class>pl.kommedia.jpa.rozrachunki.RozrachunekPro</class>
		<class>pl.kommedia.jpa.rozrachunki.SprzedazPro</class>
		<class>pl.kommedia.jpa.sprzedaz.FakturaBil</class>
		<class>pl.kommedia.jpa.sprzedaz.FakturaPodstawowaBil</class>
		
		<properties>
			<property name="eclipselink.target-server" value="JBoss"/>
			<property name="eclipselink.logging.level" value="FINE"/>
		</properties>
	</persistence-unit>
</persistence>
